# Increase volume
"pactl set-sink-volume @DEFAULT_SINK@ +5%"
   XF86AudioRaiseVolume

# Decrease volume
"pactl set-sink-volume @DEFAULT_SINK@ -5%"
   XF86AudioLowerVolume

# Mute volume
"pactl set-sink-mute @DEFAULT_SINK@ toggle"
   XF86AudioMute

# Mute microphone
"pactl set-source-mute @DEFAULT_SOURCE@ toggle"
   XF86AudioMicMute

# Increase brightness
"brightnessctl s 5%+"
    XF86MonBrightnessUp

# Decrease brightness
"brightnessctl s 5%-"
    XF86MonBrightnessDown

# Pulsemixer
"st -e pulsemixer"
    Mod4 + F12

# Lf
"st -e lf"
    Mod4 + e

# Fetch
"st -e pfetch"
    Mod4 + Super_L
